@article{schnellerDesignRhetoricStudying2015,
  title = {Design {{Rhetoric}}: {{Studying}} the {{Effects}} of {{Designed Objects}}},
  shorttitle = {Design {{Rhetoric}}},
  author = {Schneller, Annina},
  date = {2015},
  journaltitle = {Nature and Culture},
  volume = {10},
  number = {3},
